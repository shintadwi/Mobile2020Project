<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.shinta.mobile2020project.HomeFragment"
        android:label="@string/home_frag"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.shinta.mobile2020project.DashboardFragment"
        android:label="@string/dashboard_frag"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notification"
        android:name="com.shinta.mobile2020project.NotificationFragment"
        android:label="@string/notification_frag"
        tools:layout="@layout/fragment_notification" />

</navigation>